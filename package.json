{
  "name": "galaxy-client",
  "version": "0.1.0",
  "main": "src/index.js",
  "files": [
    "README.rst",
    "LICENSE.rst",
    "src/*"
  ],
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-lib": "vue-cli-service build --target lib --name galaxy-client src/index.js",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@vuex-orm/core": "^0.34.1",
    "@vuex-orm/plugin-axios": "^0.9.2",
    "byte-size": "^6.2.0",
    "lodash.merge": "^4.6.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.1.2",
    "@vue/cli-plugin-eslint": "^4.1.2",
    "@vue/cli-service": "^4.1.2",
    "babel-eslint": "^10.0.3",
    "eslint": "^6.8.0",
    "eslint-plugin-vue": "^6.1.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-prototype-builtins": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
